{if question.hasInFields}
	{loop question.whereFields as whereField}
		{if whereField.isInField}
			List.of({whereField.sqlTestValue})
		{else}
			{whereField.sqlTestValue}
		{endif}
		{if !whereFieldStatus.last}, {endif}
	{endloop}
{else}
	{loop question.whereFields as whereField}
		{whereField.sqlTestValue}
		{if !whereFieldStatus.last}, {endif}
	{endloop}
{endif}